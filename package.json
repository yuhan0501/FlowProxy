{
  "name": "flowproxy",
  "version": "1.0.0",
  "description": "A desktop proxy tool with visual flow orchestration",
  "main": "dist/main/main/main.js",
  "build": {
    "appId": "com.flowproxy.app",
    "productName": "FlowProxy",
    "directories": {
      "output": "dist"
    },
    "files": [
      "dist/main/**/*",
      "dist/renderer/**/*",
      "package.json"
    ],
    "mac": {
      "target": "dmg",
      "category": "public.app-category.developer-tools"
    }
  },
  "scripts": {
    "start": "npm run build && electron .",
    "dev": "concurrently \"npm run dev:main\" \"npm run dev:renderer\"",
    "dev:main": "tsc -p tsconfig.main.json && electron .",
    "dev:renderer": "webpack serve --config webpack.renderer.config.js --mode development",
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "tsc -p tsconfig.main.json",
    "build:renderer": "webpack --config webpack.renderer.config.js --mode production",
    "clean": "rm -rf dist",
    "dist": "npm run build && electron-builder --mac dmg"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/http-proxy": "^1.17.14",
    "@types/node": "^20.10.0",
    "@types/node-forge": "^1.3.10",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/uuid": "^9.0.7",
    "concurrently": "^8.2.2",
    "css-loader": "^6.8.1",
    "electron": "^28.0.0",
    "electron-builder": "^26.0.12",
    "html-webpack-plugin": "^5.5.4",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@monaco-editor/react": "^4.6.0",
    "ace-builds": "^1.27.0",
    "antd": "^5.12.1",
    "http-proxy": "^1.18.1",
    "node-forge": "^1.3.1",
    "react": "^18.2.0",
    "react-ace": "^11.0.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "reactflow": "^11.10.1",
    "uuid": "^9.0.1",
    "vm2": "^3.9.19"
  }
}
